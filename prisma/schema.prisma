generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  admin    Boolean @default(false)
}

model historique_coureurs {
  id            Int     @id @default(autoincrement())
  classement    Int?
  coureur       String
  num_coureur   Int?
  id_equipe     Int?
  temps         String?
  ecart         String?
  annee         Int?
  distance      Int?
  nbr_etape     Int?
  type_result   String?
  total_seconde Int?
  ecart_seconde Int?
}

model etapes {
  id           Int      @id @default(autoincrement())
  numero_etape Int
  date         DateTime
  depart       String
  arrivee      String
  distance     Float
  type         String
  denivele     Int
  difficulte   String
  description  String?
}